{
  "name": "blockmass",
  "version": "0.21.32",
  "private": true,
  "scripts": {
    "predev": "npm run version:patch",
    "version:patch": "node scripts/bump-version.mjs patch && node scripts/sync-version.mjs",
    "version:minor": "node scripts/bump-version.mjs minor && node scripts/sync-version.mjs",
    "version:major": "node scripts/bump-version.mjs major && node scripts/sync-version.mjs",
    "setup:env": "node scripts/setup-env.mjs",
    "dev": "next dev --turbopack -p 5555",
    "build": "next build",
    "start": "next start -p 5555"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.3.0",
    "mongodb": "^6.20.0",
    "mongoose": "^8.18.2",
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "three": "^0.180.0"
  },
  "devDependencies": {
    "@types/react": "19.2.0",
    "typescript": "5.9.3"
  }
}
